/**
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
  IN a, b, sel;
  OUT out;

  PARTS:
    Not(in=sel, out=notsel);
    And(a=notsel, b=a, out=anda);
    And(a=sel, b=b, out=andb);
    Or(a=anda, b=andb, out=out);
}